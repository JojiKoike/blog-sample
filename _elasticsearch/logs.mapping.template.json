{
    "template": "view*",
    "settings": {
        "number_of_shards" : 5,
        "number_of_replicas" : 1
    },
    "mappings": {
        "_default_": {
            "dynamic_templates": [
                {
                    "keyword_template" : {
                        "match" : "*",
                        "mapping": { "type": "keyword", "index": "not_analyzed" },
                        "match_mapping_type" : "keyword"
                    }
                }
            ],
            "properties" : {
                "url" : { "type":"keyword", "index": "not_analyzed" },
                "user_id" : { "type": "integer" },
                "@timestamp" : { "type" : "date", "index" : "not_analyzed" }
            }
        }
    }
}
